+ */
+int PrefixData::GetSwKeyCount()
+{
+    int swKeyCount = 0; 
+
+    uint8_t zeroKey[ECDSA521_KEY_SIZE];
+    memset( zeroKey, 0, ECDSA521_KEY_SIZE );
+
+    if (memcmp( m_swPkeyP, zeroKey, ECDSA521_KEY_SIZE) != 0)
+    {
+        ++swKeyCount;
