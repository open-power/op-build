+class IBM_HexString
+{
+    friend std::ostream& operator<< ( std::ostream& strm,
+                                      const IBM_HexString& hs );
+
+public:
+    inline void setWidth( uint8_t width )
+    { 
+        m_width = width;
+    }
+
+    inline void setLeadSpace( uint8_t leadSpace )
+    { 
